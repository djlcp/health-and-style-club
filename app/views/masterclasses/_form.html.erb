<%= form_for @masterclass do |form| %>


  <div class="form-group">
    <%= form.label :title %><br>
    <%= form.text_field :title %>
  </div>

  <div class="form-group">
    <%= form.label :text %>
    <%= form.text_field :text %>
  </div>

  <div class="form-group">
    <%= form.submit "Submit", class: "btn btn-primary btn-lg" %>
  </div>
<% end %>
