<h1>admin page</h1>

<table>
  <% @users.each do |user| %>
    <tr>
      <td><%= link_to user.email, users_path(user) %></td>
      <td>
        <%= link_to 'Add User', new_user_path %>
      </td>
      <td>
        <%= link_to 'Edit', edit_user_path(user) %>
      </td>
      <td>
        <%= link_to 'Delete', user_path(user), method: :delete, data: { confirm: "You sure?" } %>
      </td>
    </tr>
  <% end %>
</table>

<%= link_to 'Back', posts_path %>
